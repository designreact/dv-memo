{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
   },
   "globals": {
     "expect": false
   },
   "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
   },
   "rules": {
     "arrow-body-style": [0],
     "arrow-parens": [0],
     "comma-dangle": [1, "always-multiline"],
     "global-require": [0],
     "import/no-named-as-default": [0],
     "max-len": [1, 180, 4],
     "no-console": [0],
     "react/prefer-stateless-function": [0],
     "semi": [2, "never"],
     "strict": [2, "global"],
     "import/first": [0]
   }
}
